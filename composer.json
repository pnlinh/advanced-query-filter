{
    "name": "fvsoft/laravel-advanced-query-filter",
    "description": "Advanced Query Filter package for Laravel",
    "keywords": [
        "fvsort",
        "laravel-advanced-query-filter",
        "advanced-query-filter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dinh Quoc Han",
            "email": "contact@dinhquochan.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "~5.6.0|~5.7.0|~5.8.0",
        "illuminate/http": "~5.6.0|~5.7.0|~5.8.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0"
    },
    "replace": {
        "fvsoft/advanced-query-filter":  "self.version"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "FVSoft\\QueryFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FVSoft\\QueryFilter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FVSoft\\QueryFilter\\Providers\\AdvancedQueryFilterServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
